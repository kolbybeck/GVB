@model Dictionary<int, string>

@{
    ViewBag.Title = "ChooseFeedlot";
    int iCount = 1;
}

<h2>Choose Feedlot</h2>

<div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
    @foreach (var item in Model)
    {

        <div class="row">
            <h3> @item.Value   @Html.ActionLink(">>", "Create", "Deceased", new { feedlotID = iCount, feedlotName = item.Value }, new { @class = "btn btn-primary btn-large" })</h3>
        </div>
        <br />
        iCount++;
    }
</div>
